{
    "compilerOptions": {
        "checkJs": true,
        "target": "esnext",
        "module": "esnext",
        "strict": true,
        "noImplicitOverride": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noPropertyAccessFromIndexSignature": false, 
        "noImplicitAny": false,
        "baseUrl": ".",
        "paths": {
            "resource:///org/gnome/shell/*": ["../gnome-shell/js/*"]
        }
    }
}
