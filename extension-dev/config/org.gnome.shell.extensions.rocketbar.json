{
    "taskbar-enabled": true,
    "taskbar-show-favorites": true,
    "taskbar-isolate-workspaces": true,
    "taskbar-show-all-windows": true,
    "taskbar-enable-separator": true,
    "appbutton-enable-tooltips": true,
    "appbutton-enable-indicators": true,
    "appbutton-enable-notification-badges": true,
    "appbutton-enable-progress-bars": true,
    "appbutton-enable-sound-control": true,
    "taskbar-position": "left",
    "taskbar-position-offset": 1,
    "taskbar-preserve-position": false,
    "appbutton-icon-size": 20,
    "appbutton-icon-padding": 8,
    "appbutton-icon-vertical-padding": 3,
    "appbutton-spacing": 1,
    "appbutton-roundness": 50,
    "appbutton-backlight-color": "rgb(255, 255, 255)",
    "appbutton-backlight-dominant-color": false,
    "appbutton-backlight-intensity": 1,
    "indicator-dominant-color-active": false,
    "indicator-color-active": "rgb(53, 132, 228)",
    "indicator-dominant-color-inactive": false,
    "indicator-color-inactive": "rgb(255, 255, 255)",
    "indicator-position": "top",
    "indicator-width-inactive": 4,
    "indicator-width-active": 4,
    "indicator-height-inactive": 4,
    "indicator-height-active": 4,
    "indicator-spacing-inactive": 2,
    "indicator-spacing-active": 2,
    "indicator-margin-inactive": 0,
    "indicator-margin-active": 0,
    "indicator-display-limit": 3,
    "indicator-display-limit-active": 3,
    "notification-counter-enabled": true,
    "notification-counter-hide-empty": false,
    "notification-counter-center-clock": true,
    "notification-counter-max-count": 999,
    "notification-counter-font-size": 10,
    "notification-counter-roundness": 10,
    "notification-counter-margin-top": -1,
    "notification-counter-color-empty": "white",
    "notification-counter-color-not-empty": "white",
    "notification-counter-text-color": "rgb(0, 0, 0)",
    "notification-counter-color-empty-dnd": "rgba(255, 255, 255, 0.8)",
    "notification-counter-color-not-empty-dnd": "rgba(255, 255, 255, 0.8)",
    "notification-counter-text-color-dnd": "rgb(0, 0, 0)",
    "notification-service-enable-unity-dbus": true
}
